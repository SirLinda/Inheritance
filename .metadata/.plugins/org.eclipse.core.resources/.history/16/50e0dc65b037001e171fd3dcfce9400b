import java.util.Scanner;

public class main {
	
	public static void printBoard(final int[][] myBoard, int widthAndHieght) {
		
		for(int i=0; i < widthAndHieght; i++) {
			for(int j=0; j < widthAndHieght; j++) {
				System.out.print(myBoard[i][j]);
			}
			System.out.print("\n");
		}
		
	}

